<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Syncfusion.Maui.ToolKit.BottomSheet.SfBottomSheetStyle"
             xmlns:bottomsheet="clr-namespace:Syncfusion.Maui.Toolkit.BottomSheet">

    <Style TargetType="bottomsheet:SfBottomSheet">
        <Setter Property="Background" Value="{DynamicResource SfBottomSheetBackground}" />
        <Setter Property="GrabberBackground" Value="{DynamicResource SfBottomSheetGrabberBackground}" />
    </Style>

</ResourceDictionary>